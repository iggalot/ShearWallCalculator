<UserControl x:Class="ShearWallVisualizer.Controls.ShearWallResultsControl_Rigid"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ShearWallVisualizer.Controls"
             mc:Ignorable="d" 
             >
    <StackPanel Orientation="Vertical">
        <StackPanel Orientation="Horizontal" Background="AliceBlue">
            <Label FontSize="10" Width="50" Height="25" Background="LightGray" Margin="5 1 5 1" FontWeight="Bold" Name="ID"/>
            <Label FontSize="10" Width="50" Height="25" Background="LightGray" Margin="5 1 5 1" Name="Rgidity"/>
            <Label FontSize="10" Width="50" Height="25" Background="LightGray" Margin="5 1 5 1" Name="Xbar"/>
            <Label FontSize="10" Width="50" Height="25" Background="LightGray" Margin="5 1 5 1" Name="Ybar"/>
            <Label FontSize="10" Width="50" Height="25" Background="LightGray" Margin="5 1 5 1" Name="Vix"/>
            <Label FontSize="10" Width="50" Height="25" Background="LightGray" Margin="5 1 5 1" Name="Viy"/>
            <Label FontSize="10" Width="50" Height="25" Background="LightGray" Margin="5 1 5 1" Name="V_ecc"/>
            <Label FontSize="10" Width="50" Height="25" Background="LightGray" Margin="5 1 5 1" FontWeight="Bold" Name="V_tot"/>
        </StackPanel>
    </StackPanel>
</UserControl>
